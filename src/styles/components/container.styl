.container
  display flex
  min-height 100vh

.main-container
  align-items flex-start
  align-self flex-start
  background-color $container-background
  border-radius 5px
  box-shadow -1px -1px 2px 0px rgba(50, 50, 50, 0.5);
  display flex
  flex-flow row wrap
  margin 35px 20px 20px 0px
  padding 5px
  width 90%
